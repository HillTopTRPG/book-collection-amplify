    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.6",
    "typescript": "~5.5.4",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint:fix"
    ]
  },
  "overrides": {
    "@aws-sdk/types": "3.887.0",
    "form-data": "2.5.5",
    "tough-cookie": "4.1.4",
    "@xstate/react": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "uuid": "$uuid",
    "glob": "^10.0.0",
    "rimraf": "^5.0.0"
  }
}
