{
  "name": "amplify-vite-react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite --host",
    "dev": "vite --host",
    "dev:mock": "VITE_MOCK_MODE=true vite --host",
    "build": "tsc & vite build & wait",
    "lint": "eslint . && tsc --noEmit",
    "lint:fix": "prettier --write --log-level=silent . && eslint . --fix",
    "lint:suppress": "eslint --suppress-all --fix",
    "preview": "vite preview",
    "prepare": "husky",
    "sandbox": "npx ampx sandbox",
    "sandbox:delete": "npx ampx sandbox delete",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "gh:clean-ci": "./scripts/delete-workflow-runs.sh 'CI and Quality Checks' 10",
    "gh:clean-ci-batch": "./scripts/delete-workflow-runs.sh 'CI and Quality Checks' 30",
    "gh:list-runs": "gh run list --workflow='CI and Quality Checks' --limit 20"
  },
  "dependencies": {
    "@aws-amplify/data-schema-types": "^1.2.0",
    "@aws-amplify/ui-react": "^6.12.1",
    "@dnd-kit/core": "^6.3.1",
